            <corners android:radius="25dp"></corners>
            <stroke android:width="1dp" android:color="@color/couleurBienvenue" />
            <solid android:color="@color/colAddListButton_vccabFrag"></solid>
        </shape>
    </item>
    <item android:state_focused="true">
        <shape android:shape="rectangle">
            <corners android:radius="25dp"></corners>
            <stroke android:width="1dp" android:color="@color/colAddListButton_vccabFrag" />
            <solid android:color="@color/colAddListButton_vccabFrag"/>
        </shape>
    </item>
    <item >
        <shape android:shape="rectangle">
            <corners android:radius="25dp"></corners>
            <stroke android:width="1dp" android:color="@color/colAddListButton_vccabFrag" />
            <solid android:color="@color/colAddListButton_vccabFrag"/>
        </shape>
    </item>
</selector>